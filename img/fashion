	    // 分享到朋友圈
	    wx.onMenuShareTimeline({
	      title: '时尚生活', // 商品名
	      link: 'http://chengdumote.com/c/fashion', // 商品购买地址
	      imgUrl: 'http://source.chengdumote.com/wp-content/themes/sugarbox/images/weixinii.jpg', // 分享的图标
	      fail: function (res) {
	        alert(JSON.stringify(res));
	      }
	    });
	});
</script>
